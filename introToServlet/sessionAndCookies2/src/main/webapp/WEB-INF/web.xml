<web-app xmlns= "https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation= "https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" version= "5.0" metadata-complete= "true">




<!--    Sess -->
    <servlet>
        <servlet-name>LogIn</servlet-name>
        <servlet-class>com.serv.Session.log</servlet-class>

    </servlet>
    <servlet-mapping>
        <servlet-name>LogIn</servlet-name>
        <url-pattern>/log</url-pattern>
    </servlet-mapping>
    
    
<!--    //HomePage-->
    <servlet>
        <servlet-name>HomePage</servlet-name>
        <servlet-class>com.serv.Session.HomePage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomePage</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>


<!-- ///logout -->
    <servlet>
        <servlet-name>logedOut</servlet-name>
        <servlet-class>com.serv.Session.logOut</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logedOut</servlet-name>
        <url-pattern>/Out</url-pattern>
    </servlet-mapping>





    <!--    //logError-->
    <servlet>
        <servlet-name>error</servlet-name>
        <servlet-class>com.serv.Session.error</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>error</servlet-name>
        <url-pattern>/error</url-pattern>
    </servlet-mapping>







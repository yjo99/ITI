    </servlet>
    <servlet-mapping>
        <servlet-name>HomePage</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>


    <!--    //HomePage-->
    <servlet>
        <servlet-name>ckCookie</servlet-name>

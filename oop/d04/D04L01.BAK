#include <stdio.h>
#include <conio.h>




void swap(int *x, int *y);



int main(){

   int x, y;

   clrscr();

   printf("Enter the first number: \n");
   scanf("%d", &x);

   printf("Enter the sec number: \n");
   scanf("%d", &y);


   printf("before swaping\n");


   printf("x : %d, y: %d\n",x,y);

   swap(&x,&y);


   printf("After swaping\n");


   printf("x : %d, y: %d\n",x,y);

   getch();

}



void swap( int *x, int *y){

	 int temp;

	 temp = *x;
	 *x = *y;
	 *y = temp;



}